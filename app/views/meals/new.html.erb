<div id="provide_meal">

<%= form_for @meal do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>
  <%= f.label :street %>
  <%= f.text_field :street %>
  <%= f.label :street_number %>
  <%= f.text_field :street_number %>
  <%= f.label :time %>
  <%= f.text_field :time, :class => "datepicker" %>
  <%= f.label :deadline %>
  <%= f.text_field :deadline, :class => "datepicker" %>

  <%= f.hidden_field(:lat) %>
  <%= f.hidden_field(:lon) %>
  
  <%= f.submit 'submit', :class => "submit", :onclick => "return:false;" %>
<% end %>

<div id="map" style="width: 450px; height: 300px"></div>
</div>
